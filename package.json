{
  "name": "api",
  "version": "1.0.0",
  "description": "Github Viewer - API",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --runInBand --detectOpenHandles --silent --noStackTrace",
    "test:v": "jest --passWithNoTests --runInBand --detectOpenHandles --verbose",
    "test:w": "npm run test -- --watch",
    "test:u": "npm run test -- -c jest.unit.config.ts",
    "test:i": "npm run test -- -c jest.integration.config.ts",
    "test:ci": "npm run test -- --coverage",
    "test:ci:u": "npm run test:u -- --coverage",
    "test:ci:i": "npm run test:i -- --coverage",
    "test:staged": "npm test -- --findRelatedTests",
    "lint": "eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "18.4.2",
    "@commitlint/config-conventional": "18.4.2",
    "@jest/types": "29.6.3",
    "@types/jest": "29.5.8",
    "@types/node": "20.8.2",
    "@types/supertest": "2.0.16",
    "commitlint": "18.4.2",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "16.3.1",
    "gts": "^5.2.0",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "lefthook": "1.5.2",
    "lint-staged": "15.1.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.2.0"
  },
  "commitlint": {
    "extends": [
      "./node_modules/@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts}": [
      "npm run lint"
    ]
  },
  "engines": {
    "node": ">=10.6.0"
  }
}